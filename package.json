{
  "name": "xournalpp-preview",
  "displayName": "xournalpp-preview",
  "description": "Previews Xournalpp .xopp files (read-only)",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loglob/vscode-xournalpp-preview"
  },
  "categories": [
    "Other"
  ],
  "publisher": "loglob",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "xournalpp-preview.editor",
        "displayName": "Xournalpp Document",
        "selector": [
          { "filenamePattern": "*.xopp" },
          { "filenamePattern": "*.xoj" }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "./node_modules/.bin/vsce package"
  },
  "devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.76.0",
		"typescript": "^4.9.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.18.0"
  }
}
